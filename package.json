{
  "name": "iciba-translate-userscript",
  "version": "4.0.0",
  "description": "iciba划词翻译",
  "main": "server.js",
  "scripts": {
    "dev": "npm run clean:dist && node ./build/dev-server.js",
    "watch": "webpack --config ./config/webpack.dev.conf.js --watch",
    "build": "npm run lint && webpack --config ./config/webpack.prod.conf.js && ts-node -P ./utils/tsconfig-util.json ./utils/addMetaData.ts && npm run clean",
    "a": "npm run analyze",
    "ac": "npm run analyze:cat",
    "analyze": "webpack --config ./config/webpack.prod.analyze.conf.js",
    "analyze:cat": "webpack --config ./config/webpack.prod.analyze-concatenated.conf.js",
    "clean": "ts-node -P ./utils/tsconfig-util.json ./utils/apiBuildCleanup.ts",
    "clean:dist": "ts-node -P ./utils/tsconfig-util.json ./utils/apiDistCleanup.ts",
    "lint": "ts-node -P ./utils/tsconfig-util.json ./build/lint.ts",
    "lint:eslint": "eslint --ext \"js,ts,vue\" ./src/",
    "lint:tslint": "node ./lintRules/tslint/compile.js && tslint -t verbose -p ./tsconfig.json ./src/** | ts-node -P ./utils/tsconfig-util.json utils/tslintFormatter.ts",
    "lint:css": "stylelint  ./**/*.less ./**/*.vue"
  },
  "keywords": [
    "iciba",
    "typescript",
    "userscript",
    "translate"
  ],
  "author": "noe132",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@babel/runtime-corejs2": "^7.0.0",
    "typescript": "^3.0.3",
    "vue": "^2.5.17",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^7.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@types/event-emitter": "^0.3.2",
    "@types/greasemonkey": "^0.0.28",
    "@types/node": "^10.11.0",
    "@types/rimraf": "^2.0.2",
    "babel-loader": "^8.0.2",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsdoc": "^3.8.0",
    "eslint-plugin-typescript": "^0.12.0",
    "eslint-plugin-vue": "^4.7.1",
    "express": "^4.16.3",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "rimraf": "^2.6.2",
    "speed-measure-webpack-plugin": "^1.2.3",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-processor-html": "^1.0.0",
    "ts-loader": "^5.2.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-consistent-codestyle": "^1.13.3",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "^3.6.0",
    "typescript-eslint-parser": "^18.0.0",
    "url-loader": "^1.1.1",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-client": "^4.1.1",
    "webpack-merge": "^4.1.4"
  }
}
