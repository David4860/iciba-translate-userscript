{
  "name": "iciba-translate-userscript",
  "version": "4.0.0",
  "description": "iciba划词翻译",
  "main": "server.js",
  "scripts": {
    "dev": "npm run clean:dist && node ./build/dev-server.js",
    "watch": "webpack --config ./config/webpack.dev.conf.js --watch",
    "build": "npm run lint && webpack --config ./config/webpack.prod.conf.js && ts-node -P ./utils/tsconfig-util.json ./utils/addMetaData.ts && npm run clean",
    "a": "npm run analyze",
    "ac": "npm run analyze:cat",
    "analyze": "webpack --config ./config/webpack.prod.analyze.conf.js",
    "analyze:cat": "webpack --config ./config/webpack.prod.analyze-concatenated.conf.js",
    "clean": "ts-node -P ./utils/tsconfig-util.json ./utils/apiBuildCleanup.ts",
    "clean:dist": "ts-node -P ./utils/tsconfig-util.json ./utils/apiDistCleanup.ts",
    "lint": "ts-node -P ./utils/tsconfig-util.json ./build/lint.ts",
    "lint:eslint": "eslint --ext \"js,ts,vue\" ./src/",
    "lint:tslint": "node ./tslintRules/compile.js && tslint -t verbose -p ./tsconfig.json ./src/** | ts-node -P ./utils/tsconfig-util.json utils/tslintFormatter.ts",
    "lint:css": "stylelint  ./**/*.less ./**/*.vue"
  },
  "keywords": [
    "iciba",
    "typescript",
    "userscript",
    "translate"
  ],
  "author": "noe132",
  "license": "MIT",
  "dependencies": {
    "google-translate-token": "^1.0.0",
    "typescript": "^2.9.2",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^6.1.0"
  },
  "devDependencies": {
    "@types/greasemonkey": "^0.0.28",
    "@types/node": "^10.3.5",
    "@types/rimraf": "^2.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.1",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-typescript": "^0.12.0",
    "eslint-plugin-vue": "^4.5.0",
    "express": "^4.16.3",
    "less": "^3.0.2",
    "less-loader": "^4.1.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-processor-html": "^1.0.0",
    "ts-loader": "^4.4.1",
    "ts-node": "^7.0.0",
    "tslint": "^5.9.1",
    "tslint-consistent-codestyle": "^1.13.1",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-loader": "^3.6.0",
    "typescript-eslint-parser": "^16.0.0",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.12.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-client": "^4.0.3",
    "webpack-merge": "^4.1.3"
  }
}
