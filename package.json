{
  "name": "iciba-translate-userscript",
  "version": "4.0.0",
  "description": "iciba划词翻译",
  "main": "server.js",
  "scripts": {
    "dev": "npm run clean:dist && node ./build/dev-server.js",
    "watch": "webpack --config ./config/webpack.dev.conf.js --watch",
    "build": "npm run lint && webpack --config ./config/webpack.prod.conf.js && ts-node -P ./utils/tsconfig-util.json ./utils/addMetaData.ts && npm run clean",
    "a": "npm run analyze",
    "analyze": "webpack --config ./config/webpack.prod.analyze.conf.js",
    "clean": "ts-node -P ./utils/tsconfig-util.json ./utils/apiBuildCleanup.ts",
    "clean:dist": "ts-node -P ./utils/tsconfig-util.json ./utils/apiDistCleanup.ts",
    "lint": "ts-node -P ./utils/tsconfig-util.json ./build/lint.ts",
    "lint:eslint": "eslint --ext \"js,ts,vue\" ./src/",
    "lint:tslint": "node ./lintRules/tslint/compile.js && tslint -t verbose -p ./tsconfig.json ./src/**/*.ts | ts-node -P ./utils/tsconfig-util.json utils/tslintFormatter.ts",
    "lint:css": "stylelint  ./**/*.less ./**/*.vue"
  },
  "keywords": [
    "iciba",
    "typescript",
    "userscript",
    "translate"
  ],
  "author": "noe132",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@babel/runtime-corejs2": "^7.4.3",
    "@noe132/vue-style-loader": "^4.2.0",
    "io-ts": "^1.8.5",
    "material-design-icons-iconfont": "^4.0.5",
    "stylus-loader": "^3.0.2",
    "typescript": "^3.4.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-property-decorator": "^8.1.0",
    "vuetify": "^1.5.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@types/event-emitter": "^0.3.3",
    "@types/greasemonkey": "^4.0.0",
    "@types/node": "^11.13.0",
    "@types/rimraf": "^2.0.2",
    "@types/tampermonkey": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsdoc": "^4.8.1",
    "eslint-plugin-vue": "^5.2.2",
    "express": "^4.16.4",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "rimraf": "^2.6.3",
    "speed-measure-webpack-plugin": "^1.3.1",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-processor-html": "^1.0.0",
    "stylus": "^0.54.5",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "^3.6.0",
    "url-loader": "^1.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.2.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-client": "^4.1.1",
    "webpack-merge": "^4.2.1"
  }
}
