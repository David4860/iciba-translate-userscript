{
  "name": "iciba-translate-userscript",
  "version": "4.0.0",
  "description": "iciba划词翻译",
  "main": "server.js",
  "scripts": {
    "dev": "npm run clean:dist && node ./build/dev-server.js",
    "watch": "webpack --config ./config/webpack.dev.conf.js --watch",
    "build": "npm run lint && webpack --config ./config/webpack.prod.conf.js && ts-node -P ./utils/tsconfig-util.json ./utils/addMetaData.ts && npm run clean",
    "a": "npm run analyze",
    "ac": "npm run analyze:cat",
    "analyze": "webpack --config ./config/webpack.prod.analyze.conf.js",
    "analyze:cat": "webpack --config ./config/webpack.prod.analyze-concatenated.conf.js",
    "clean": "ts-node -P ./utils/tsconfig-util.json ./utils/apiBuildCleanup.ts",
    "clean:dist": "ts-node -P ./utils/tsconfig-util.json ./utils/apiDistCleanup.ts",
    "lint": "ts-node -P ./utils/tsconfig-util.json ./build/lint.ts",
    "lint:eslint": "eslint --ext \"js,ts,vue\" ./src/",
    "lint:tslint": "node ./tslintRules/compile.js && tslint -t verbose -p ./tsconfig.json ./src/** | ts-node -P ./utils/tsconfig-util.json utils/tslintFormatter.ts",
    "lint:css": "stylelint ./src"
  },
  "keywords": [
    "iciba",
    "typescript",
    "userscript",
    "translate"
  ],
  "author": "noe132",
  "license": "MIT",
  "dependencies": {
    "google-translate-token": "^1.0.0",
    "typescript": "^2.8.3",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^6.0.0"
  },
  "devDependencies": {
    "@types/greasemonkey": "^0.0.28",
    "@types/md5": "^2.1.32",
    "@types/node": "^10.0.0",
    "@types/rimraf": "^2.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-minify": "^0.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-typescript": "^0.12.0",
    "eslint-plugin-vue": "^4.5.0",
    "express": "^4.16.3",
    "fast-async": "^6.3.7",
    "less": "^3.0.2",
    "less-loader": "^4.1.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-processor-html": "^1.0.0",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-consistent-codestyle": "^1.13.0",
    "tslint-loader": "^3.6.0",
    "typescript-eslint-parser": "^15.0.0",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.0.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.15",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-client": "^2.2.2",
    "webpack-merge": "^4.1.2"
  }
}
