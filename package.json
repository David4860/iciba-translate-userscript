{
  "name": "iciba-translate-userscript",
  "version": "4.0.0",
  "description": "iciba划词翻译",
  "main": "server.js",
  "scripts": {
    "build": "npm run lint:build && npm run lint:css && npm run webpack:prod && ts-node -P tsconfig-util.json ./utils/addMetaData.ts && npm run clean",
    "watch": "npm run webpack:dev:watch",
    "dev": "npm run webpack:dev",
    "a": "npm run analyze",
    "analyze": "webpack --config ./config/webpack.prod.analyze.conf.js",
    "webpack:dev": "webpack --config ./config/webpack.dev.conf.js",
    "webpack:dev:watch": "webpack --config ./config/webpack.dev.conf.js --watch",
    "webpack:prod": "webpack --config ./config/webpack.prod.conf.js",
    "clean": "ts-node -P tsconfig-util.json ./utils/apiBuildCleanup.ts",
    "lint": "npm run lint:tslint --silent || npm run lint:eslint --silent || echo linting finished",
    "lint:build": "npm run lint:tslint --silent && npm run lint:eslint --silent",
    "lint:eslint": "eslint --ext \"js,ts,vue\" ./src/",
    "lint:tslint": "node ./tslintRules/compile.js && tslint -p ./tsconfig-util.json ./src/** ./config/** | ts-node -P tsconfig-util.json utils/tslintFormatter.ts",
    "lint:css": "stylelint ./src"
  },
  "keywords": [],
  "author": "noe132",
  "license": "ISC",
  "dependencies": {
    "google-translate-token": "^1.0.0",
    "md5": "^2.2.1",
    "typescript": "^2.8.3",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-stage-0": "^7.0.0-beta.46",
    "@babel/runtime": "^7.0.0-beta.46",
    "@types/greasemonkey": "^0.0.28",
    "@types/md5": "^2.1.32",
    "@types/node": "^10.0.0",
    "@types/rimraf": "^2.0.2",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.2",
    "chalk": "^2.4.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-typescript": "^0.12.0",
    "eslint-plugin-vue": "^4.5.0",
    "happypack": "^5.0.0",
    "less": "^3.0.2",
    "less-loader": "^4.1.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "stylelint": "^9.2.0",
    "stylelint-processor-html": "^1.0.0",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-consistent-codestyle": "^1.13.0",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-loader": "^3.6.0",
    "typescript-eslint-parser": "^15.0.0",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.0.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.15",
    "webpack-merge": "^4.1.2"
  }
}
